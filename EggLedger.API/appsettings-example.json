{
  "Kestrel:Certificates:Development:Password": "{YOUR-LONG-PASSWORD}",
  "Jwt:SecretKey": "{YOUR-LONG-SECURE-KEY}",
  "Jwt:Issuer": "EggLedgerAPI",
  "Jwt:ExpiryInMinutes": "15",
  "Jwt:Audience": "EggLedgerAudience",
  "ConnectionStrings:DefaultConnection": "Host={your-db-container-name};Port=5432;Username={your-username};Password={your-password};Database=EggLedgerDB;",
  "Authentication:Google:ClientSecret": "{YOUR-LONG-SECURE-CLIENT-SECRET}",
  "Authentication:Google:ClientId": "{YOUR-LONG-SECURE-CLIENT-ID}",
  "Environment": {
    "EGGLEDGER_LOG_PATH": {
      "Windows": "C:\\Logs\\EggLedger",
      "Linux": "/var/log/eggledger",
      "macOS": "/var/log/eggledger"
    }
  }
}